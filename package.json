{
  "name": "photocarousel",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "react-dev": "webpack -d --watch",
    "test": "jest",
    "start": "nodemon index.js",
    "seed": "node ./server/db/seed.js",
    "tables": "node ./server/db/index.js",
    "start:docker": "npm run tables && npm run seed && npm run start"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFiles": [
      "./client/src/setupTests.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guest-ly/PhotoCarousel.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guest-ly/PhotoCarousel/issues"
  },
  "homepage": "https://github.com/guest-ly/PhotoCarousel#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-static-gzip": "^1.1.3",
    "faker": "^4.1.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "path": "^0.12.7",
    "sequelize": "^5.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  }
}
